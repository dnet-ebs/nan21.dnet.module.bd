<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

	<changeSet author="amathe" id="TBL">
		<createTable tableName="BD_ATTRSET_ATTR">
			<column name="ACTIVE" type="int(1)">
				<constraints nullable="false"/>
			</column>
			<column name="CLIENTID" type="varchar(64)">
				<constraints nullable="false"/>
			</column>
			<column name="CREATEDAT" type="datetime">
				<constraints nullable="false"/>
			</column>
			<column name="CREATEDBY" type="varchar(32)">
				<constraints nullable="false"/>
			</column>
			<column name="ID" type="varchar(64)">
				<constraints nullable="false"/>
			</column>
			<column name="INDESCRIPTION" type="int(1)">
				<constraints nullable="false"/>
			</column>
			<column name="LISTOFVALUES" type="text"/>
			<column name="MODIFIEDAT" type="datetime">
				<constraints nullable="false"/>
			</column>
			<column name="MODIFIEDBY" type="varchar(32)">
				<constraints nullable="false"/>
			</column>
			<column name="NOTES" type="text"/>
			<column name="REFID" type="varchar(64)">
				<constraints nullable="false"/>
			</column>
			<column name="SEQUENCENO" type="int(10)"/>
			<column name="VERSION" type="bigint(10)"/>
			<column name="ATTRIBUTESET_ID" type="varchar(64)">
				<constraints nullable="false"/>
			</column>
			<column name="ATTRIBUTESUBSET_ID" type="varchar(64)">
				<constraints nullable="false"/>
			</column>
			<column name="ATTRIBUTE_ID" type="varchar(64)">
				<constraints nullable="false"/>
			</column>
		</createTable>
	</changeSet>

	<changeSet author="amathe" id="PK">
		<addPrimaryKey 
			constraintName="PK_BD_ATTRSET_ATTR"
			tableName="BD_ATTRSET_ATTR"
			columnNames="ID"/>
	</changeSet>
	
	<changeSet author="amathe" id="UK-1">
		<addUniqueConstraint
			constraintName="UK_BD_ATTRSET_ATTR1"
			tableName="BD_ATTRSET_ATTR"
			columnNames="CLIENTID,ATTRIBUTESET_ID,ATTRIBUTE_ID"/>
	</changeSet>

</databaseChangeLog>
